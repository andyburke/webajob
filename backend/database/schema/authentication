create table credential (
  sourceid		varchar		PRIMARY KEY,
  key			varchar		NOT NULL
);

create table session (
  token			varchar		PRIMARY KEY,
  sourceid		varchar,
  starttime		int NOT NULL,
  endtime		int NOT NULL
);

